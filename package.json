{
  "name": "livechat-ccb",
  "version": "1.0.0",
  "description": "LiveChat backend",
  "main": "index.js",
  "scripts": {
    "dev": "pnpm migration:up && tsx ./src/index.ts | pnpm pino-pretty",
    "start": "node ./dist/src/index.js",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint src --ext .ts --fix",
    "prepare": "husky install",
    "generate": "prisma generate",
    "migration:up": "prisma migrate deploy",
    "migration:make": "prisma migrate dev"
  },
  "author": "qlaffont",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "18.6.1",
    "@commitlint/config-conventional": "18.6.2",
    "@fastify/cors": "9.0.1",
    "@types/node": "20.11.17",
    "@typescript-eslint/eslint-plugin": "7.0.1",
    "@typescript-eslint/parser": "7.0.1",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "fastify-socket.io": "5.0.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "pino-pretty": "10.3.1",
    "prettier": "3.2.5",
    "tsx": "^4.7.1",
    "types-fastify-socket.io": "0.0.1",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@gquittet/graceful-server": "^4.0.8",
    "@livechat/overlay-protocol": "file:packages/overlay-protocol",
    "@prisma/client": "^5.10.2",
    "@socket.io/postgres-emitter": "^0.1.0",
    "@t3-oss/env-core": "^0.8.0",
    "date-fns": "^3.3.1",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.2",
    "fastify": "^4.26.1",
    "file-type": "^19.0.0",
    "get-video-duration": "^4.1.0",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "node-fetch": "^3.3.2",
    "prisma": "^5.10.2",
    "reflect-metadata": "^0.2.1",
    "regenerator-runtime": "^0.14.1",
    "rosetty": "^3.0.22",
    "socket.io": "^4.7.4",
    "tslib": "^2.6.2",
    "unify-errors": "^1.2.202",
    "unify-fastify": "^1.4.24",
    "zlib-sync": "^0.1.9",
    "zod": "^3.22.4",
    "zod-validation-error": "^3.0.1"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8",
    "utf-8-validate": "^6.0.3"
  },
  "pnpm": {
    "overrides": {
      "undici": "^6.23.0"
    },
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "prisma"
    ]
  },
  "packageManager": "pnpm@10.29.2"
}
